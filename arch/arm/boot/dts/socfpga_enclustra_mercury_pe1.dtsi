&i2c1 {

	24aa128@57 {
		status = "okay";
		compatible = "microchip,24c128";
		reg = <0x57>;
		pagesize = <64>;
		label = "user eeprom";
		address-width = <16>;
	};

	lm96080@2f {
		status = "okay";
		compatible = "national,lm80";
		reg = <0x2f>;
	};

	si5338@70 {
		/* No driver available */
		compatible = "silabs,si5338";
		reg = <0x70>;
	};

};
